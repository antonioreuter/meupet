<div class='table-responsive'>
  <table class='table table-hover'>
    <tr>
      <th>Documento</th>
      <th>Nome</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Usuário desde</th>
      <th>Última atualização</th>
      <th>Status</th>
    </tr>

    <% @users.each do |user| %>
      <tr>
        <td><%= user.document %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= user.status %></td>
      </tr>
    <% end %>
  </table>
</div>
